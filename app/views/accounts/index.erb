<h3><% if flash.has?(:message) %>
<%= flash[:message] %>
<% end %></h3>

<% @accounts.each do |account| %>
  <button><a href="/accounts/<%= account.id %>/show"><%= account.name %></a></button><br>
  <%= account.category %><br>
<% end %>
<button><a href="/accounts/new">Open a New Account</a></button>
